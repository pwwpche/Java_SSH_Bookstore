<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="BookStore.Entity.WrittenbyEntity" table="writtenby" schema="" catalog="bookstore2">
        <cache usage="read-write"/>
        <composite-id mapped="true" class="BookStore.Entity.WrittenbyEntityPK">
            <key-property name="isbn" column="ISBN"/>
            <key-property name="aid" column="aid"/>
        </composite-id>
    </class>
</hibernate-mapping>
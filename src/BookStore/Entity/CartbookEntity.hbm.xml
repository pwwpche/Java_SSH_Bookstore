<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="BookStore.Entity.CartbookEntity" table="cartbook" schema="" catalog="bookstore">
        <composite-id mapped="true" class="BookStore.Entity.CartbookEntityPK">
            <key-property name="cid" column="cid"/>
            <key-property name="isbn" column="ISBN"/>
        </composite-id>
        <property name="quantity" column="quantity"/>
    </class>
</hibernate-mapping>